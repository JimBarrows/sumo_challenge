{
  "name": "sumo_challenge",
  "version": "0.1.0",
  "description": "A free slack app clone",
  "main": "index.js",
  "scripts": {
    "build": "webpack --progress --colors --watch -d --config react-ui/webpack.config.js",
    "start": "docker run --rm --name sumo_challenge --link mongo:mongo -v \"$PWD\":/usr/src/app -w /usr/src/app -p 3000:3000 node:6 nodemon index.js --exec babel-node",
    "start-local": "nodemon index.js --exec babel-node",
    "test:ui": "wdio tests/ui/wdio.conf.js",
    "test:api": "mocha -t 100000 --compilers js:babel-core/register tests/api/*.spec.js",
    "update-webdriver": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimBarrows/sumo_challenge.git"
  },
  "author": "Jim Barrows",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JimBarrows/sumo_challenge/issues"
  },
  "homepage": "https://github.com/JimBarrows/sumo_challenge#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "bootstrap-react-components": "^0.17.2",
    "bson-objectid": "^1.1.5",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.3.1",
    "debug": "^3.0.1",
    "express": "^4.15.4",
    "jsonwebtoken": "^8.0.0",
    "less-middleware": "^2.2.1",
    "moment": "^2.18.1",
    "mongoose": "^4.11.10",
    "morgan": "^1.8.2",
    "nodemon": "^1.12.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.2.1",
    "pug": "^2.0.0-rc.4",
    "react": "^15.6.1",
    "react-datetime": "^2.10.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-templates-and-utils": "^0.4.1",
    "redux": "^3.7.2",
    "redux-auth-wrapper": "^2.0.1",
    "redux-devtools": "^3.4.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.5.6"
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "mocha": "^3.5.0",
    "wdio-mocha-framework": "^0.5.11",
    "wdio-spec-reporter": "^0.1.2",
    "webdriverio": "^4.8.0"
  }
}
